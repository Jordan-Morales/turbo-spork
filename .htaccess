RewriteEngine On

RewriteCond %{REQUEST_METHOD} ^GET$
RewriteRule ^launches$ controllers/launch.php?action=index

RewriteCond %{REQUEST_METHOD} ^POST$
RewriteRule ^launches$ controllers/launch.php?action=post

RewriteCond %{REQUEST_METHOD} ^PUT$
RewriteRule ^launches/([0-9]+)$ controllers/launch.php?action=update&id=$1

RewriteCond %{REQUEST_METHOD} ^DELETE$
RewriteRule ^launches/([0-9]+)$ controllers/launch.php?action=delete&id=$1
